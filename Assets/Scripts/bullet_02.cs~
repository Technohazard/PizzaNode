// Bullet_02 - physics based
public class bullet_02 : MonoBehaviour {
	
	private Vector2 velocity;

	// Use this for initialization
	void Start () {
		velocity = rigidbody2D.velocity;
	}
	
	// Update is called once per frame
	void Update () {
		// test for collision
		velocity = rigidbody2D.velocity;
	}
}
